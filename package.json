{
    "name": "iw-complaint-manager",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "build": "remix vite:build",
        "dev": "HTTPS=true SSL_CRT_FILE=.cert/localhost.pem SSL_KEY_FILE=.cert/localhost-key.pem remix vite:dev --host",
        "format": "prettier --write .",
        "lint": "eslint . --cache --cache-location ./node_modules/.cache/eslint",
        "lint:fix": "eslint . --fix --cache --cache-location ./node_modules/.cache/eslint",
        "start": "yarn db:setup && node server.js",
        "db:setup": "bash ./scripts/db_setup.sh",
        "typecheck": "tsc",
        "cy:open": "cypress open",
        "cy:run": "cypress run --browser=chrome",
        "test": "yarn vitest",
        "test:ui": "yarn vitest --ui",
        "storybook": "storybook dev -p 6006 --https --ssl-cert .cert/localhost.pem --ssl-key .cert/localhost-key.pem",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@fontsource/material-icons": "^5.0.18",
        "@fontsource/roboto": "^5.0.13",
        "@mui/material": "^5.15.18",
        "@remix-run/express": "^2.9.1",
        "@remix-run/node": "^2.9.1",
        "@remix-run/react": "^2.9.1",
        "@storybook/addon-themes": "^8.1.2",
        "cross-env": "^7.0.3",
        "express": "^4.19.2",
        "init": "^0.1.2",
        "isbot": "^4.1.0",
        "pg": "^8.11.5",
        "pg-hstore": "^2.3.4",
        "react": "19.0.0-rc-d3ce0d3ea9-20240520",
        "react-dom": "19.0.0-rc-d3ce0d3ea9-20240520",
        "sequelize": "^6.37.3",
        "storybook": "^8.1.2"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^1.4.0",
        "@remix-run/dev": "^2.9.1",
        "@storybook/addon-essentials": "^8.1.2",
        "@storybook/addon-interactions": "^8.1.2",
        "@storybook/addon-links": "^8.1.2",
        "@storybook/addon-onboarding": "^8.1.2",
        "@storybook/blocks": "^8.1.2",
        "@storybook/builder-vite": "^8.1.2",
        "@storybook/react": "^8.1.2",
        "@storybook/react-vite": "^8.1.2",
        "@storybook/test": "^8.1.2",
        "@testing-library/react": "^15.0.7",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "@vitest/ui": "^1.6.0",
        "cypress": "^13.9.0",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.8.0",
        "jsdom": "^24.0.0",
        "prettier": "^3.2.5",
        "typescript": "^5.1.6",
        "vite": "^5.1.0",
        "vite-tsconfig-paths": "^4.2.1",
        "vitest": "^1.6.0"
    },
    "engines": {
        "node": "20.13.1"
    },
    "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
